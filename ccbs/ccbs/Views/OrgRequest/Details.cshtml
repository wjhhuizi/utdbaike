@model ccbs.Models.OrgRequest
@{
    ViewBag.Title = "Request Details";
}
<div id="content">
    <fieldset>
        <legend>Request Details</legend>
        <div class="display-label">
            From:</div>
        <div class="display-field">
            <p>@Model.Organization.Name</p>
        </div>
        <div class="display-label">
            Date:</div>
        <div class="display-field">
            <p>@Model.RequestDate</p>
        </div>
        <div class="display-label">
            Number of New Students</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.NumOfNews)
        </div>
        <div class="display-label">
            Note</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Note)
        </div>
        <div class="display-label">
            Reply</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Reply)
        </div>
        <div class="display-label">
            Progress</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Progress)
        </div>
    </fieldset>
</div>
<div>
    @{Html.RenderPartial("_Sidebar");}
</div>
